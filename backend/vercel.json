{
  "version": 2,
  "functions": {
    "src/server.ts": {
      "runtime": "@vercel/node@18.19.0",
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/src/server.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "ALLOWED_ORIGINS": "https://restaurant-frontend-yvss.vercel.app,https://restaurant-frontend-bice.vercel.app,http://localhost:3000,http://localhost:4200"
  }
}
