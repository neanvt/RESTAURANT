{
  "version": 2,
  "functions": {
    "src/server.ts": {
      "runtime": "nodejs18.x",
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/src/server.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "$comment": "Force fresh deployment - runtime fix applied"
}